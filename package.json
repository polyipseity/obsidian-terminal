    "@types/node": "^25.1.0",
    "@types/semver": "^7.7.1",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "esbuild-compress": "^2.0.2",
    "esbuild-plugin-globals": "^0.2.0",
    "esbuild-plugin-text-replace": "^1.3.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.20.0",
    "p-lazy": "^5.0.0",
    "prettier": "^3.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "files": [
    "main.js",
    "manifest.json",
    "styles.css"
  ],
  "funding": [
    {
      "type": "Buy Me a Coffee",
			"url": "https://buymeacoffee.com/polyipseity"
		},
		{
			"type": "GitHub Sponsors",
      "url": "https://github.com/sponsors/polyipseity"
    }
  ],
  "homepage": "https://github.com/polyipseity/PLACEHOLDER#readme",
  "keywords": [
    "PLACEHOLDER",
    "obsidian",
    "obsidian-plugin",
    "plugin"
  ],
  "license": "MIT",
  "main": "main.js",
  "name": "PLACEHOLDER",
  "obsidian": {
    "authorUrl": "https://github.com/polyipseity",
    "id": "PLACEHOLDER",
    "isDesktopOnly": false,
    "minAppVersion": "1.4.11",
    "name": "PLACEHOLDER"
  },
  "overrides": {
    "tmp": "^0.2.5"
  },
  "pnpm": {
    "overrides": {
      "tmp": "^0.2.5"
    }
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polyipseity/PLACEHOLDER.git"
  },
  "scripts": {
    "build": "npm run check && npm run build:force",
    "build:dev": "npm run build:force -- dev",
    "build:force": "node build/build.mjs",
    "check": "npm run check:eslint && npm run check:md && npm run check:prettier",
    "check:eslint": "eslint --cache --max-warnings=0",
    "check:md": "markdownlint-cli2",
    "check:prettier": "prettier --check \"**/*.{astro,cjs,css,csv,gql,graphql,hbs,html,js,jsx,json,json5,jsonc,jsonl,less,mjs,pcss,sass,scss,svelte,styl,ts,tsx,vue,xml,yaml,yml}\"",
    "commitlint": "commitlint --from=origin/main --to=HEAD",
    "format": "npm run format:eslint && npm run format:md && npm run format:prettier",
    "format:eslint": "eslint --cache --fix",
    "format:md": "markdownlint-cli2 --fix",
    "format:prettier": "prettier --write \"**/*.{astro,cjs,css,csv,gql,graphql,hbs,html,js,jsx,json,json5,jsonc,jsonl,less,mjs,pcss,sass,scss,svelte,styl,ts,tsx,vue,xml,yaml,yml}\"",
    "obsidian:install": "npm run build && node build/obsidian-install.mjs",
    "obsidian:install:force": "npm run build:force && node build/obsidian-install.mjs",
    "postversion": "node build/version-post.mjs",
    "prepare": "husky",
    "version": "node build/version.mjs"
  },
  "sideEffects": false,
  "style": "styles.css",
  "type": "module",
  "version": "0.0.0",
  "workspaces": [
    ".",
    "build/*"
  ]
}

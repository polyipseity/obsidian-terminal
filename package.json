    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/compat": "^2.0.2",
    "@eslint/js": "^9.39.2",
		"@polyipseity/obsidian": "~1.4.11",
    "@tsconfig/node16": "^16.1.8",
    "@tsconfig/recommended": "^1.0.13",
    "@tsconfig/strictest": "^2.0.8",

    "builtin-modules": "^5.0.0",
    "esbuild": "^0.27.2",
    "esbuild-compress": "^2.0.2",
    "esbuild-plugin-globals": "^0.2.0",
    "esbuild-plugin-text-replace": "^1.3.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdownlint-cli2": "^0.20.0",
    "p-lazy": "^5.0.0",
    "prettier": "^3.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
